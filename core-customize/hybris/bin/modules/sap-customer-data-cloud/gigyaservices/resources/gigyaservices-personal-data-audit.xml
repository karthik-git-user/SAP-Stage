<audit-report-config name="PersonalDataReport">
    <given-root-type>User</given-root-type>
    <types>
        <type code="User" mode="append">
            <atomic-attributes>
                <atomic-attribute qualifier="gyUID" displayName="gyUID"/>                
                <atomic-attribute qualifier="gyIsOriginGigya" displayName="gyIsOriginGigya"/>
                <atomic-attribute qualifier="gyCreated" displayName="gyCreated"/>
                <atomic-attribute qualifier="gyIsLockedOut" displayName="gyIsLockedOut"/>
                <atomic-attribute qualifier="gyIsRegistered" displayName="gyIsRegistered"/>
                <atomic-attribute qualifier="gyIsVerified" displayName="gyIsVerified"/>
                <atomic-attribute qualifier="gyLoginProvider" displayName="gyLoginProvider"/>
                <atomic-attribute qualifier="gyRegistered" displayName="gyRegistered"/>
                <atomic-attribute qualifier="gySocialProviders" displayName="gySocialProviders"/>
                <atomic-attribute qualifier="gyVerified" displayName="verified"/>
                <atomic-attribute qualifier="gyDataCenter" displayName="gyDataCenter"/>
            </atomic-attributes>
        </type>
    </types>
</audit-report-config>